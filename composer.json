{
	"name": "david63/friendsandfoes",
	"type": "phpbb-extension",
	"description": "Adds an Admin option to display the friends & foes of each member (where set).",
	"version": "2.1.0-RC1",
	"time": "2017-01-07",
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "David Wood",
			"role": "Developer"
		}
	],
	"require": {
        "php": ">=5.4.0",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "[*] Friends and foes",
		"soft-require": {
        	"phpbb/phpbb": ">=3.2.0"
      },
	  "version-check": {
      	"host": "www.phpbb.com",
        "directory": "/customise/db/extension/friends_foes",
        "filename": "version_check"
	  },
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/3.2",
        	"filename": "/friends_and_foes_version_file.json"
    	}
	}
}
